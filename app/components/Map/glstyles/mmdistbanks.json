{
  "version": 8,
  "sources": {
    "osm-mmdistbanks-raw": {
      "type": "vector",
      "tiles": [
        "{{server}}/mmdistbanks/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 13,
      "maxzoom": 14
    },
    "osm-mmdistbanks-aggregated": {
      "type": "vector",
      "tiles": [
        "{{server}}/mmdistbanks/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 0,
      "maxzoom": 12
    }
  },
  "layers": [
    {
      "id": "mmdistbanks-raw",
      "type": "fill",
      "source": "osm-mmdistbanks-raw",
      "source-layer": "osm",
      "paint": {
        "fill-color": "#510000",
        "fill-opacity": 1,
        "fill-outline-color": "#E08214"
      },
      "filter": [
        "==",
        "$type",
        "Polygon"
      ]
    },
    {
      "id": "mmdistbanks-raw-road",
      "type": "line",
      "source": "osm-mmdistbanks-raw",
      "source-layer": "osm",
      "paint": {
        "line-width": 1,
        "line-color": "#9e9ac8",
        "line-opacity": 1
      },
      "filter": [
        "==",
        "$type",
        "LineString"
      ]
    },
    {
      "id": "mmdistbanks-raw-mm",
      "type": "circle",
      "source": "osm-mmdistbanks-raw",
      "source-layer": "osm",
      "paint": {
        "circle-color": "#FF0000",
        "circle-opacity": 1
      },
      "filter": [
        "all",
        [
          "==",
          "$type",
          "Point"
        ],
        [
          "==",
          "amenity",
          "mobile_money_agent"
        ]
      ]
    },
    {
      "id": "mmdistbanks-raw-bank",
      "type": "circle",
      "source": "osm-mmdistbanks-raw",
      "source-layer": "osm",
      "paint": {
        "circle-color": "#0000FF",
        "circle-opacity": 1
      },
      "filter": [
        "all",
        [
          "==",
          "$type",
          "Point"
        ],
        [
          "==",
          "amenity",
          "mobile_money_agent"
        ]
      ]
    },
    {
      "id": "mmdistbanks-raw-highlight",
      "type": "fill",
      "source": "osm-mmdistbanks-raw",
      "source-layer": "osm",
      "paint": {
        "fill-color": "#5CBAD8",
        "fill-opacity": 1
      },
      "filter": [
        "==",
        "_timestamp",
        -1
      ]
    },
    {
      "id": "mmdistbanks-aggregated-0",
