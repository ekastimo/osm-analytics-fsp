{
  "version": 8,
  "sources": {
    "osm-fspdistribution-raw": {
      "type": "vector",
      "tiles": [
        "{{server}}/fspdistribution/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 13,
      "maxzoom": 14
    },
    "osm-fspdistribution-aggregated": {
      "type": "vector",
      "tiles": [
        "{{server}}/fspdistribution/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 0,
      "maxzoom": 12
    },
    "osm-population-raw": {
      "type": "vector",
      "tiles": [
        "{{server}}/population/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 0,
      "maxzoom": 14
    }
  },
  "layers": [
    {
      "id": "population-raw-ditribution",
      "_description":"Building",
      "type": "fill",
      "source": "osm-population-raw",
      "source-layer": "osm",
      "paint": {
        "fill-color": "#FDB863",
        "fill-opacity": 1,
        "fill-outline-color": "#E08214"
      }
    },
    {
      "id": "fspdistribution-raw-building",
      "_description":"Building",
      "type": "fill",
      "source": "osm-fspdistribution-raw",
      "source-layer": "osm",
      "paint": {
        "fill-color": "#FDB863",
        "fill-opacity": 1,
        "fill-outline-color": "#E08214"
      },
      "filter": [
        "==",
        "$type",
        "Polygon"
      ]
    },
    {
      "id": "fspdistribution-raw-road",
      "_description":"Road",
      "type": "line",
      "source": "osm-fspdistribution-raw",
      "source-layer": "osm",
      "paint": {
        "line-width": 1,
        "line-color": "#9e9ac8",
        "line-opacity": 1
      },
      "filter": [
        "==",
        "$type",
        "LineString"
      ]
    },
    {
      "id": "fspdistribution-raw-agent-aggregated",
      "_description":"Mobile money agent",
      "type": "circle",
      "source": "osm-fspdistribution-raw",
      "source-layer": "osm",
      "paint": {
        "circle-color": "#FF0000",
        "circle-opacity": 1
      },
      "filter": [
        "==",
        "$type",
        "Point"
      ]
    },
    {
      "id": "fspdistribution-aggregated-0",
      "type": "fill",
      "source": "osm-fspdistribution-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#ffe4b2"
      },
      "filter": [
        "all",
        [
          ">=",
          "_populationDensity",
          0
        ],
        [
          "<",
          "_populationDensity",
          25
        ]
      ]
    },
    {
      "id": "fspdistribution-aggregated-1",
      "type": "fill",
      "source": "osm-fspdistribution-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#ffdb99",
        "fill-antialias": false
      },
      "filter": [
        "all",
        [
          ">=",
          "_populationDensity",
          25
        ],
        [
          "<",
          "_populationDensity",
          50
        ]
      ]
    },
    {
      "id": "fspdistribution-aggregated-2",
      "type": "fill",
      "source": "osm-fspdistribution-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#ffd27f",
        "fill-antialias": false
      },
      "filter": [
        "all",
        [
          ">=",
          "_populationDensity",
          50
        ],
        [
          "<",
          "_populationDensity",
          75
        ]
      ]
    },
    {
      "id": "fspdistribution-aggregated-3",
      "type": "fill",
      "source": "osm-fspdistribution-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#ffc04c",
        "fill-antialias": false
      },
      "filter": [
        "all",
        [
          ">=",
          "_populationDensity",
          75
        ],
        [
          "<",
          "_populationDensity",
          100
        ]
      ]
    },

    {
      "id": "fspdistribution-aggregated-4",
      "type": "fill",
      "source": "osm-fspdistribution-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#ffae19",
        "fill-antialias": false
      },
      "filter": [
        "all",
        [
          ">=",
          "_populationDensity",
          100
        ],
        [
          "<",
          "_populationDensity",
          125
        ]
      ]
    },
    {
      "id": "fspdistribution-aggregated-5",
      "type": "fill",
      "source": "osm-fspdistribution-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#ffa500",
        "fill-antialias": false
      },
      "filter": [
        "all",
        [
          ">=",
          "_populationDensity",
          125
        ],
        [
          "<",
          "_populationDensity",
          150
        ]
      ]
    },
    {
      "id": "fspdistribution-aggregated-6",
      "type": "fill",
      "source": "osm-fspdistribution-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#cc8400",
        "fill-antialias": false
      },
      "filter": [
        "all",
        [
          ">=",
          "_populationDensity",
          150
        ],
        [
          "<",
          "_populationDensity",
          200
        ]
      ]
    },
    {
      "id": "fspdistribution-aggregated-7",
      "type": "fill",
      "source": "osm-fspdistribution-aggregated",
      "source-layer": "osm",
      "maxzoom": 12.01,
      "paint": {
        "fill-color": "#996300",
        "fill-antialias": false
      },
      "filter": [
        "all",
        [
          ">=",
          "_populationDensity",
          200
        ]
      ]
    }
    
  ]
}









